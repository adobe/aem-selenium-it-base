{
  "jcr:primaryType": "sling:Folder",
  "settings": {
    "jcr:primaryType": "sling:Folder",
    "wcm": {
      "jcr:primaryType": "cq:Page",
      "templates": {
        "jcr:primaryType": "cq:Page",
        "content-page": {
          "jcr:primaryType": "cq:Template",
          "initial": {
            "jcr:primaryType": "cq:Page",
            "jcr:content": {
              "jcr:primaryType": "cq:PageContent",
              "cq:template": "/conf/test-site/settings/wcm/templates/content-page",
              "sling:resourceType": "wcm/foundation/components/page",
              "root": {
                "jcr:primaryType": "nt:unstructured",
                "sling:resourceType": "wcm/foundation/components/responsivegrid",
                "responsivegrid": {
                  "jcr:primaryType": "nt:unstructured",
                  "sling:resourceType": "wcm/foundation/components/responsivegrid",
                  "text": {
                    "jcr:primaryType": "nt:unstructured",
                    "text":"<p>This page was generated by an automated integration test<\/p>",
                    "sling:resourceType":"foundation/components/text","textIsRich":"true"
                  }

                }
              }
            }
          },
          "jcr:content": {
            "jcr:primaryType": "cq:PageContent",
            "jcr:title": "Content Page",
            "status": "enabled",
            "jcr:description": "Default page template for test pages",
            "cq:templateType": "/conf/test-site/settings/wcm/template-types/content-page"
          },
          "structure": {
            "jcr:primaryType": "cq:Page",
            "jcr:content": {
              "jcr:primaryType": "cq:PageContent",
              "cq:template": "/conf/test-site/settings/wcm/templates/content-page",
              "cq:deviceGroups": [
                "/etc/mobile/groups/responsive"
              ],
              "sling:resourceType": "wcm/foundation/components/page",
              "root": {
                "jcr:primaryType": "nt:unstructured",
                "sling:resourceType": "wcm/foundation/components/responsivegrid",
                "responsivegrid": {
                  "jcr:primaryType": "nt:unstructured",
                  "sling:resourceType": "wcm/foundation/components/responsivegrid",
                  "editable": "true",
                  "cq:responsive": {
                    "jcr:primaryType": "nt:unstructured"
                  }
                }
              },
              "cq:responsive": {
                "jcr:primaryType": "nt:unstructured",
                "breakpoints": {
                  "jcr:primaryType": "nt:unstructured",
                  "phone": {
                    "jcr:primaryType": "nt:unstructured",
                    "title": "Smaller Screen",
                    "width": "650"
                  },
                  "tablet": {
                    "jcr:primaryType": "nt:unstructured",
                    "title": "Tablet",
                    "width": "1200"
                  }
                }
              }
            }
          },
          "policies": {
            "jcr:primaryType": "cq:Page",
            "jcr:content": {
              "jcr:primaryType": "nt:unstructured",
              "sling:resourceType": "wcm/core/components/policies/mappings",
              "root": {
                "jcr:primaryType": "nt:unstructured",
                "cq:policy": "wcm/foundation/components/responsivegrid/default-policy",
                "sling:resourceType": "wcm/core/components/policies/mapping",
                "responsivegrid": {
                  "jcr:primaryType": "nt:unstructured",
                  "cq:policy": "wcm/foundation/components/responsivegrid/default-policy",
                  "sling:resourceType": "wcm/core/components/policies/mapping",
                  "test-site": {
                    "jcr:primaryType": "nt:unstructured",
                    "components": {
                      "jcr:primaryType": "nt:unstructured",
                      "form": {
                        "jcr:primaryType": "nt:unstructured",
                        "container": {
                          "jcr:primaryType": "nt:unstructured",
                          "cq:policy": "test-site/components/form/container/form-container",
                          "sling:resourceType": "wcm/core/components/policies/mapping"
                        }
                      }
                    }
                  },
                  "cq": {
                    "jcr:primaryType": "nt:unstructured",
                    "tagging": {
                      "jcr:primaryType": "nt:unstructured",
                      "components": {
                        "jcr:primaryType": "nt:unstructured",
                        "tagcloud": {
                          "jcr:primaryType": "nt:unstructured",
                          "cq:policy": "cq/tagging/components/tagcloud/tagcloud-policy",
                          "sling:resourceType": "wcm/core/components/policies/mapping"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "policies": {
        "jcr:primaryType": "cq:Page",
        "jcr:mixinTypes": [
          "rep:AccessControllable"
        ],
        "wcm": {
          "jcr:primaryType": "nt:unstructured",
          "foundation": {
            "jcr:primaryType": "nt:unstructured",
            "components": {
              "jcr:primaryType": "nt:unstructured",
              "responsivegrid": {
                "jcr:primaryType": "nt:unstructured",
                "default-policy": {
                  "jcr:primaryType": "nt:unstructured",
                  "jcr:title": "Default test site policy",
                  "components": [
                    "group:General",
                    "group:Form"
                  ],
                  "sling:resourceType": "wcm/core/components/policy/policy",
                  "jcr:content": {
                    "jcr:primaryType": "nt:unstructured"
                  }
                }
              }
            }
          }
        },
        "cq": {
          "jcr:primaryType": "nt:unstructured",
          "tagging": {
            "jcr:primaryType": "nt:unstructured",
            "components": {
              "jcr:primaryType": "nt:unstructured",
              "tagcloud": {
                "jcr:primaryType": "nt:unstructured",
                "tagcloud-policy": {
                  "jcr:primaryType": "nt:unstructured",
                  "jcr:title": "Tag Cloud component policy",
                  "linkTemplate": "/content/test-site.html?tag=${tagid_link}",
                  "sling:resourceType": "wcm/core/components/policy/policy",
                  "jcr:content": {
                    "jcr:primaryType": "nt:unstructured"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}